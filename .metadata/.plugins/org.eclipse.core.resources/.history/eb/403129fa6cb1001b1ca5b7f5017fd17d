package ch03_codingPractice;

import java.util.Scanner;

public class Q03 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num;
		
		System.out.println("정수를 입력하시오 >> ");
		num = sc.nextInt();
		
		for(int i=0; i<num; i++) {
			for(int j=0; j<num-1; j++)
			System.out.print("*");
		}
		System.out.println();
		sc.close();
	}
}