package ch2_repractice;

public class Q2 {
	private static int sum;

	public static void main(String[] args) {
		for(int i=0; i<10; i++) sum += i;
			System.out.println(i);
		for(int i=9; i<0; i--) sum += i;
			System.out.println(i);
		for(int i=0; i>10; i++) sum ++ i;
			System.out.println(i);
		for(int i=0; i<10; sum += i. i++);
			System.out.println(i);
	}
}
