package ch2_repractice;

public class Q2 {
	public static void main(String[] args) {
		int sum = 0;
		for(int i=0; i<10; i++) sum += i;
			System.out.println(sum);
		for(int i=9; i<0; i--) sum += i;
			System.out.println(sum);
		for(int i=0; i>10; i++); 
			System.out.println(sum);
		for(int i=0; i<10;  i++);
			System.out.println(sum);
	}
}
